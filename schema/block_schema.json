{
	"$schema": "http://json-schema.org/schema#",
	"id": "MUX_block",
	"type": "object",
	"properties":{
		"ChannelType":{"type":"string","enum":["STRING","DOUBLE","BINARY"]},
		"True":{
			"type":"object",
			"properties": {
				"function":{"type":"string","enum":["fixed","variable"]},
				"var":{
					"oneOf":[
						{"type":"string"},
						{"type":"number"},
						{"type":"boolean"}
					]
				}
			},
			"required":["var"],
		},
		"False":{
			"type":"object",
			"properties": {
				"function":{"type":"string","enum":["fixed","variable"]},
				"var":{
					"oneOf":[
						{"type":"string"},
						{"type":"number"},
						{"type":"boolean"}
					]
				}
			},
			"required":["var"],
		},
		"Output":{"type":"string"},
		"Switch":{
			"type":"object",
			"properties": {
				"type":{"type":"string","enum":["BINARY","DOUBLE"]},
				"var":{"type":"string"},
				"threshold":{"type":"number"}
			},
			"required":["var"],
		}
	}
}